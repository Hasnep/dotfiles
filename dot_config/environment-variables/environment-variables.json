{
  "bash": {
    "HISTFILE": "$XDG_STATE_HOME/bash/history"
  },
  "cargo": {
    "CARGO_HOME": "$XDG_DATA_HOME/cargo",
    "PATH": {
      "priority": 1,
      "values": ["$XDG_DATA_HOME/cargo/bin/"]
    }
  },
  "docker": {
    "DOCKER_CONFIG": "$XDG_CONFIG_HOME/docker"
  },
  "elm": {
    "ELM_HOME": "$XDG_CONFIG_HOME/elm"
  },
  "gnupg": {
    "GNUPGHOME": "$XDG_DATA_HOME/gnupg"
  },
  "go": {
    "GOPATH": "$XDG_DATA_HOME/go"
  },
  "gtk-2": {
    "GTK2_RC_FILES": "$XDG_CONFIG_HOME/gtk-2.0/gtkrc"
  },
  "guix": {
    "BASH_LOADABLES_PATH": "$GUIX_PROFILE/lib/bash",
    "CHICKEN_REPOSITORY_PATH": "$GUIX_PROFILE/var/lib/chicken/11",
    "CPLUS_INCLUDE_PATH": "$GUIX_PROFILE/include/c++:$GUIX_PROFILE/include",
    "C_INCLUDE_PATH": "$GUIX_PROFILE/include",
    "GUIX_CONFIG_FOLDER": "$XDG_CONFIG_HOME/guix",
    "GUIX_DATA_FOLDER": "$XDG_DATA_HOME/guix",
    "GUIX_LOCPATH": "$GUIX_PROFILE/lib/locale",
    "GUIX_MANIFEST": "$GUIX_CONFIG_FOLDER/manifest.scm",
    "GUIX_PROFILE": "$GUIX_DATA_FOLDER/default",
    "GUIX_PYTHONPATH": "$GUIX_PROFILE/lib/python3.9/site-packages",
    "LIBRARY_PATH": "$GUIX_PROFILE/lib",
    "PATH": {
      "priority": 10,
      "values": ["$GUIX_PROFILE/bin", "$GUIX_PROFILE/sbin"]
    },
    "R_LIBS_SITE": "$GUIX_PROFILE/site-library/"
  },
  "java": {
    "_JAVA_OPTIONS": "-Djava.util.prefs.userRoot=$XDG_CONFIG_HOME/java"
  },
  "julia": {
    "JULIA_DEPOT_PATH": "$XDG_DATA_HOME/julia"
  },
  "juliaup": {
    "PATH": {
      "priority": 3,
      "values": ["$XDG_DATA_HOME/juliaup/bin/"]
    }
  },
  "kde": {
    "KDEHOME": "$XDG_CONFIG_HOME/kde"
  },
  "less": {
    "LESSHISTFILE": "$XDG_CACHE_HOME/less/history",
    "LESSKEY": "$XDG_CONFIG_HOME/less/lesskey"
  },
  "nimble": {
    "NIMBLE_DIR": "$XDG_DATA_HOME/nimble"
  },
  "nix": {
    "NIX_CONF_DIR": "$XDG_CONFIG_HOME/nix",
    "NIX_PROFILE_DIR": "$XDG_STATE_HOME/nix/profile",
    "NIX_SSH_CERT_FILE": "/etc/ssl/certs/ca-certificates.crt",
    "PATH": {
      "priority": 5,
      "values": ["$NIX_PROFILE_DIR/bin", "/nix/var/nix/profiles/default/bin"]
    }
  },
  "node": {
    "NODE_REPL_HISTORY": "$XDG_DATA_HOME/node_repl_history"
  },
  "npm": {
    "NPM_CONFIG_USERCONFIG": "$XDG_CONFIG_HOME/npm/npmrc",
    "npm_config_cache": "$XDG_CACHE_HOME/npm"
  },
  "nvidia": {
    "CUDA_CACHE_PATH": "$XDG_CACHE_HOME/nv",
    "__GL_SHADER_DISK_CACHE_PATH": "$XDG_CACHE_HOME/nv"
  },
  "opam": {
    "OPAMROOT": "$XDG_DATA_HOME/opam"
  },
  "pass": {
    "PASSWORD_STORE_DIR": "$XDG_DATA_HOME/pass"
  },
  "platformio": {
    "PLATFORMIO_CORE_DIR": "$XDG_DATA_HOME/platformio"
  },
  "python": {
    "PYTHONSTARTUP": "$XDG_CONFIG_HOME/python/pythonrc"
  },
  "ruff": {
    "RUFF_CACHE_DIR": "$XDG_CACHE_HOME/ruff"
  },
  "rustup": {
    "RUSTUP_HOME": "$XDG_DATA_HOME/rustup"
  },
  "snap": {
    "PATH": {
      "priority": 4,
      "values": ["/snap/bin"]
    }
  },
  "system-path": {
    "PATH": {
      "priority": 999,
      "values": [
        "/bin",
        "/sbin",
        "/usr/bin/",
        "/usr/games",
        "/usr/local/bin",
        "/usr/local/games",
        "/usr/local/sbin",
        "/usr/sbin"
      ]
    }
  },
  "topiary": {
    "TOPIARY_LANGUAGE_DIR": "$HOME/Documents/rust/topiary/languages"
  },
  "user-path": {
    "PATH": {
      "priority": 100,
      "values": ["$HOME/.local/bin/", "$HOME/bin/"]
    }
  },
  "wine": {
    "WINEPREFIX": "$XDG_DATA_HOME/wine"
  },
  "xdg-variables": {
    "XDG_CACHE_HOME": "$HOME/.cache",
    "XDG_CONFIG_DIRS": "/etc/xdg",
    "XDG_CONFIG_HOME": "$HOME/.config",
    "XDG_DATA_DIRS": "/usr/local/share:/usr/share:/var/lib/flatpak/exports/share:$XDG_DATA_HOME/flatpak/exports/share",
    "XDG_DATA_HOME": "$HOME/.local/share",
    "XDG_DESKTOP_DIR": "$HOME/Desktop",
    "XDG_DOCUMENTS_DIR": "$HOME/Documents",
    "XDG_DOWNLOAD_DIR": "$HOME/Downloads",
    "XDG_MUSIC_DIR": "$HOME/Music",
    "XDG_PICTURES_DIR": "$HOME/Pictures",
    "XDG_STATE_HOME": "$HOME/.local/state",
    "XDG_VIDEOS_DIR": "$HOME/Videos"
  },
  "xinit": {
    "XINITRC": "$XDG_CONFIG_HOME/x11/xinitrc"
  },
  "zsh": {
    "ZDOTDIR": "$XDG_CONFIG_HOME/zsh"
  }
}
